{
    "name": "rezzza/vaultage",
    "description": "Keep secrets secret",
    "keywords": ["secret", "configuration"],
    "type": "standalone",
    "homepage": "https://github.com/rezzza/vaultage",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/vaultage/contributors"
    }],
    "require": {
        "php": ">=5.3.3",
        "ext-mcrypt": "*",
        "symfony/options-resolver": "2.2.*",
        "symfony/console": "2.2.*",
        "symfony/process": "2.2.*",
        "sebastian/diff": "*"
    },
    "require-dev": {
        "symfony/finder": "2.1.*",
        "atoum/atoum": "dev-master"
    },
    "autoload":     {
        "psr-0": {
            "Rezzza\\Vaultage": "src/"
        }
    },
    "bin": [
        "vaultage"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
